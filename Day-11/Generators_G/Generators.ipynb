   "source": [
    "#✅ Basic Generator \n",
    "\n",
    "def squares(n):\n",
    "    for i in range(1,n+1):\n",
    "        yield i*i\n",
    "\n",
    "for val in squares(5):\n",
    "    print(val) \n",
    "\n",
    "\n",
    "    # ✅ Efficient, doesn’t store all data in memory."
   ]
  }
